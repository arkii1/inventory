{
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "semi": 0,
      "no-unused-vars":"warn",
      "prettier/prettier": [
        "error", {
          "semi": false
        }
      ],
      "consistent-return":"off",
      "func-names": "off",
      "no-underscore-dangle": "off",
      "no-restricted-syntax":"off"
    }
  }